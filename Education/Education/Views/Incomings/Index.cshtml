@model IEnumerable<Education.Data.Incoming>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutSchool.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
